<!-- Residents collection -->
<%- include('partials/header') %>

<div style="  padding: 4%;padding-top: 0%;font-family: sans-serif;font-size: 30px;">
  <h2 style="background-color:rgb(51, 51, 51); padding:20px;color:white; text-align: center; border-radius: 3rem; font-size: 25px;">
      Residents of <%= societyName %>
  </h2>
  <div class="flex-container">
    <% societyResidents.forEach(function(user){ %>
      <div>
        <ul class="list-group zoom">
          <li class="list-group-item flat-number">
            <i class="fas fa-house-user"></i> <%= user.flatNumber %>
          </li>
          <li class="list-group-item flat-owner"><%= user.firstName %> <%= user.lastName %></li>
          <li class="list-group-item" style="font-size: 17px;">
            <i class="fas fa-phone-square-alt"></i> <%= user.phoneNumber %>
          </li>
        </ul>
      </div>
    <% }) %>
  </div>
</div>
  
<%- include('partials/footer') %>